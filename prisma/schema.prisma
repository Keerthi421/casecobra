generator client {
  provider = "prisma-client-js"
  // output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Configarator {
  id              String  @id @default(cuid())
  height          Int
  width           Int
  imageUrl        String
  croppedImageUrl String?
}

model Hiiiiii {
  id          String   @id @default(cuid())
  name        String
  description String?
  imageUrl    String
  createdAt   DateTime @default(now())
}
